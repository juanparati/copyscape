{
    "name": "juanparati/copyscape",
    "description": "Copyscape interface for Laravel",
    "type": "library",
    "keywords": [
        "copyscape",
        "laravel",
        "plagiarism",
        "duplicate",
        "seo"
    ],
    "require": {
        "php": ">=7.3",
        "ext-simplexml": "*",
        "ext-json": "*",
        "guzzlehttp/guzzle": "~7.0",
        "illuminate/support": "~8.0|~9.0"
    },
    "autoload": {
        "psr-4": {
            "Juanparati\\Copyscape\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Juanparati\\Copyscape\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Juanparati\\Copyscape\\CopyscapeServiceProvider"
            ]
        }
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Juan Lago",
            "email": "juanparati@gmail.com"
        }
    ],
    "minimum-stability": "dev"
}
